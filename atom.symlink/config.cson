"*":
  editor:
    fontFamily: "Source Code Pro"
    fontSize: 12
    invisibles:
      {}
    preferredLineLength: 999
    softWrap: true
  core:
    disabledPackages: [
      "color-picker"
      "minimap"
      "neon-cursor"
      "autocomplete-jedi"
    ]
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  "release-notes":
    viewedVersion: "0.95.0"
  welcome:
    showOnStartup: false
  "git-diff":
    showIconsInEditorGutter: true
  linter:
    {}
  jsformat:
    keep_array_indentation: true
    format_on_save: false
    jslint_happy: true
  "autocomplete-plus":
    {}
  Zen:
    width: 120
  "isotope-ui":
    fontFamily: "Roboto"
  "vim-mode":
    startInInsertMode: true
    useClipboardAsDefaultRegister: true
  esformatter:
    {}
  "tool-bar":
    position: "Left"
    visible: false
  "exception-reporting":
    userId: "771fbd7e-00e9-4e40-1fc0-3a07e1be039a"
  "atom-color-highlight":
    {}
  pigments:
    {}
  "xml-formatter":
    endLineCharacter: "LF (\\n)"
    xmlUtf8Header: false
  "one-light-ui":
    {}
".source.sql":
  editor:
    preferredLineLength: 999
